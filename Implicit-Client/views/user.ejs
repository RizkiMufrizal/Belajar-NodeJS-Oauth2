<h1 class="text-center">Data User</h1>

<table class="table table-hover table-striped table-bordered table-responsive">
  <thead>
    <tr>
      <th>Id User</th>
      <th>Username</th>
      <th>Password</th>
    </tr>
  </thead>
  <tbody ng-repeat="u in user.dataUser">
    <tr>
      <td>{{u._id}}</td>
      <td>{{u.username}}</td>
      <td>{{u.password}}</td>
    </tr>
  </tbody>
</table>
